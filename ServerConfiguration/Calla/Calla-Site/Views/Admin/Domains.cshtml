@{
    Layout = "_AdminPage";
    ViewData["Title"] = "Referrer Domains";
}

@model StringCount[]


@if (Model.Length == 0)
{
    <p>No Content</p>
}
else
{
    <ul>
        @foreach(var row in Model)
        {
            <li>
                [@row.Count] @row.Value
            </li>
        }
    </ul>
}